          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  UNIX_TIMESTAMP(cm.commit_timestamp) as time_sec,\n  c.modified_coverage as value\nFROM _tool_codecov_comparisons c\nINNER JOIN _tool_codecov_commits cm ON c.connection_id = cm.connection_id AND c.repo_id = cm.repo_id AND c.commit_sha = cm.commit_sha\nWHERE c.connection_id = ${connection_id}\n  AND c.repo_id IN (${repo_id})\n  AND (c.flag_name IN (${flag_name}) OR c.flag_name = '')\n  AND cm.commit_timestamp >= $__timeFrom()\n  AND cm.commit_timestamp <= $__timeTo()\n  AND c.modified_coverage IS NOT NULL\nUNION ALL\nSELECT\n  UNIX_TIMESTAMP(commit_timestamp) as time_sec,\n  modified_coverage as value\nFROM _tool_codecov_commit_coverages\nWHERE connection_id = ${connection_id}\n  AND repo_id IN (${repo_id})\n  AND modified_coverage IS NOT NULL\n  AND commit_timestamp >= $__timeFrom()\n  AND commit_timestamp <= $__timeTo()\nORDER BY time_sec",
          "refId": "A",
          "timeColumn": "commit_timestamp"
